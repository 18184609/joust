div#winners , div#consolation { 
    margin-right:auto; 
    margin-left:auto; 
}


#navmenu ul {
    margin: 0; 
    padding: 0; 
    list-style-type: none; 
    list-style-image: none; 
}

#navmenu li {
    display: inline; 
    margin: .25em;
}

#navmenu ul li a {
        text-decoration:none; 
}

#navmenu ul li a:hover {
    color: purple;
    background: yellow; 
}



table.game-table {
    border-collapse:collapse;
}

table.game-table td {
    vertical-align: text-top;
    text-align: left;
    border: 1px solid gray;
}

table.game-table td.victor {
 background: #FF9;   
}

table#next-up td {
    vertical-align: text-top;
    text-align: left;
    border: 2px solid green;
}


tr.on-deck td {

    background: blue;
    
}


table.results { border-spacing:1em .5em;}

table.results th { }
table.results td { 
text-align: center;
}

table.awards {
  border-collapse: collapse;
}

table.awards td {
  border: 0;
  margin: .25em;
  padding: .25em;;
  vertical-align: text-top;
  text-align: left;

}

table.awards th {
    text-align: right;
    vertical-align: text-top;
    color: #000000;
    font-size: 13px;
    font-weight: bold;
    margin: .25em;
    padding: .25em;;
}

table.long-awards th {
    text-align: left;
    vertical-align: text-top;
    color: #000000;
    font-size: 13px;
    font-weight: bold;
    margin: .25em;
    padding: .25em;;
}



table.scores { border-spacing:1em .5em;}
table.scores td {   text-align: center; }



tr.cgame td {background-color:pink;}



dt { float: left; clear: left; width: 200px; text-align: right; font-weight: bold; color: green; padding-right:.5em; } 
dt:after { content: ":"; }
dd { margin: 0 0 0 110px; padding: 0 0 0.5em 0; }

fieldset#fieldset-tournament {border:0;}